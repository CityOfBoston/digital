{
  "name": "@cityofboston/react-fleet",
  "version": "0.0.0",
  "description": "Components for using Fleet in our webapps",
  "private": true,
  "license": "CC0-1.0",
  "main": "build/react-fleet.es5.js",
  "module": "build/react-fleet.js",
  "types": "build/react-fleet.d.ts",
  "sideEffects": false,
  "scripts": {
    "watch": "concurrently \"yarn run build:babel -- --watch\"  \"tsc-watch --emitDeclarationOnly\" \"rollup -c -w\"",
    "storybook": "start-storybook -p 9001",
    "prebuild": "rimraf build",
    "build": "concurrently \"yarn run build:typescript\" \"yarn run build:babel && rollup -c\"",
    "build:typescript": "tsc --emitDeclarationOnly",
    "build:babel": "cross-env BABEL_ENV=esm babel src --ignore '**/*.stories.*','**/*.test.*' --out-dir build --extensions \".ts,.tsx\"",
    "prepare": "yarn run build",
    "test": "jest",
    "percy": "scripts/percy.js",
    "fetch-templates": "ts-node ./scripts/fetch-templates.ts"
  },
  "jest": {
    "snapshotSerializers": ["jest-emotion"],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/scripts/"
    ]
  },
  "dependencies": {
    "core-js": "^3.19.3"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.10",
    "@babel/runtime": "^7.6.0",
    "detect-browser": "^3.0.1",
    "prop-types": "^15.6.0",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "react-dropzone": "^8.1.0",
    "string-hash": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/runtime": "^7.1.2",
    "@cityofboston/config-babel": "^0.0.0",
    "@cityofboston/config-typescript": "^0.0.0",
    "@cityofboston/percy-common": "^0.0.0",
    "@cityofboston/storybook-common": "^0.0.0",
    "@emotion/core": "^10.0.10",
    "@percy/storybook": "^3.0.2",
    "@storybook/addon-a11y": "^5.0.10",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-knobs": "^5.0.10",
    "@storybook/addon-storyshots": "^5.0.10",
    "@storybook/addon-viewport": "^5.0.10",
    "@storybook/addons": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@types/cheerio": "^0.22.7",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "24.x.x",
    "@types/node-fetch": "^2.1.2",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.4",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-storyshots": "^3.4.8",
    "@types/storybook__react": "^4.0.1",
    "babel-core": "^7.0.0-0",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.2.0",
    "detect-browser": "^3.0.1",
    "fs-extra": "^5.0.0",
    "jest": "^24.8.0",
    "jest-emotion": "^10.0.10",
    "node-fetch": "^2.6.1",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "react-dropzone": "^8.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.1",
    "string-hash": "^1.1.3",
    "ts-node": "^6.0.3",
    "typescript": "^4.4.2"
  }
}
