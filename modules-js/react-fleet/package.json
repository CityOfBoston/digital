{
  "name": "@cityofboston/react-fleet",
  "version": "0.0.0",
  "description": "Components for using Fleet in our webapps",
  "private": true,
  "license": "CC0-1.0",
  "main": "build/react-fleet.es5.js",
  "module": "build/react-fleet.js",
  "types": "build/react-fleet.d.ts",
  "sideEffects": false,
  "scripts": {
    "watch": "concurrently \"npm run build:babel -- --watch\"  \"tsc-watch --emitDeclarationOnly\" \"rollup -c -w\"",
    "storybook": "start-storybook -p 9001",
    "prebuild": "rimraf build",
    "build": "concurrently \"npm run build:typescript\" \"npm run build:babel && rollup -c\"",
    "build:typescript": "tsc --emitDeclarationOnly",
    "build:babel": "cross-env BABEL_ENV=esm babel src --ignore '**/*.stories.*','**/*.test.*' --out-dir build --extensions \".ts,.tsx\"",
    "prepare": "npm run build",
    "pretest": "tsc --noEmit",
    "test": "scripts/test.js",
    "fetch-templates": "ts-node ./scripts/fetch-templates.ts"
  },
  "jest": {
    "preset": "@cityofboston/config-jest-babel"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@babel/runtime": "7.0.0",
    "detect-browser": "^3.0.1",
    "emotion": "9.2.10",
    "prop-types": "^15.6.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "string-hash": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@cityofboston/config-babel": "^0.0.0",
    "@cityofboston/config-jest-babel": "^0.0.0",
    "@cityofboston/config-typescript": "^0.0.0",
    "@cityofboston/percy-common": "^0.0.0",
    "@percy-io/percy-storybook": "^1.4.1",
    "@storybook/addon-actions": "4.0.0-alpha.24",
    "@storybook/addon-storyshots": "4.0.0-alpha.24",
    "@storybook/addons": "4.0.0-alpha.24",
    "@storybook/react": "4.0.0-alpha.24",
    "@types/cheerio": "^0.22.7",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "23.x.x",
    "@types/node-fetch": "^1.6.9",
    "@types/react": "^16.4.7",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-storyshots": "^3.4.1",
    "@types/storybook__react": "^3.0.7",
    "babel-core": "^7.0.0-0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.5",
    "detect-browser": "^3.0.1",
    "emotion": "9.2.10",
    "fs-extra": "^5.0.0",
    "jest": "23.6.0",
    "node-fetch": "^1.6.9",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.1",
    "string-hash": "^1.1.3",
    "ts-node": "^6.0.3",
    "typescript": "^3.0.0"
  }
}
