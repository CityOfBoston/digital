    "boom": "^7.2.0",
    "classlist-polyfill": "^1.2.0",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "dataloader": "^1.3.0",
    "dotenv": "^5.0.0",
    "elasticsearch": "^14.0.0",
    "emotion": "^8.0.9",
    "emotion-server": "^8.0.9",
    "fs-extra": "^4.0.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "hapi": "^17.5.2",
    "hapi-accept-language2": "^2.0.2",
    "html5-history-api": "^4.2.8",
    "http-aws-es": "^4.0.0",
    "http-status": "^1.0.1",
    "https-proxy-agent": "^2.0.0",
    "inert": "^5.1.0",
    "isomorphic-fetch": "^2.2.1",
