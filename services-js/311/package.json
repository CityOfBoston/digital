{
  "name": "311",
  "version": "0.0.0",
  "repository": "https://github.com/CityOfBoston/311.git",
  "license": "CC0-1.0",
  "engines": {
    "node": "7.5.0"
  },
  "scripts": {
    "dev": "babel-watch -D -w server server",
    "test": "jest",
    "build": "./scripts/build_server.sh",
    "heroku-postbuild": "./scripts/build_server.sh",
    "start": "node build/server",
    "storybook": "start-storybook -p 9001 -c storybook",
    "lint": "eslint --fix .",
    "generate-schema": "babel-node ./scripts/generate-schema",
    "generate-graphql-flow": "apollo-codegen generate data/graphql/*.graphql --schema graphql/schema.json --target flow --output data/graphql/schema.flow.js",
    "clear-babel-cache": "rm -rf node_modules/.cache/babel-loader",
    "precommit": "eslint --quiet .",
    "prepush": "flow check --quiet && jest --silent --no-cache --noStackTrace",
    "fetch-salesforce-access-token": "node ./scripts/fetch-salesforce-access-token",
    "fetch-templates": "babel-node ./scripts/fetch-templates"
  },
  "jest": {
    "resetMocks": true,
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/.next/",
      "<rootDir>/coverage/"
    ],
    "collectCoverageFrom": [
      "{components,data,lib,pages,server}/**/*\\.js",
      "!**/*.stories.js"
    ],
    "coverageReporters": [
      "lcov",
      "json"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@newrelic/native-metrics": "^2.1.0",
    "babel-runtime": "^6.22.0",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "fs-extra": "^2.0.0",
    "glamor": "^2.20.22",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.1",
    "graphql": "^0.9.1",
    "graphql-server-hapi": "^0.6.0",
    "graphql-tools": "^0.10.0",
    "hapi": "^16.1.0",
    "https-proxy-agent": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mobx": "^3.1.5",
    "mobx-react": "^4.1.2",
    "mobx-utils": "^2.0.1",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "newrelic": "^1.37.2",
    "next": "2.0.0-beta.36",
    "opbeat": "^4.11.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-scoped-error-component": "^0.9.1",
    "rollbar": "^0.6.5",
    "url-search-params": "^0.6.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "apollo-codegen": "^0.10.8",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-watch": "^2.0.6",
    "cheerio": "^0.22.0",
    "codecov": "^1.0.1",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "fetch-mock": "^5.9.4",
    "flow-bin": "^0.41.0",
    "husky": "^0.13.1",
    "jest": "^19.0.2",
    "jsverify": "^0.7.4",
    "prompt": "^1.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "webpack-bundle-analyzer": "^2.3.0"
  }
}
