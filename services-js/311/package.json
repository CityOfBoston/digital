{
  "name": "311",
  "version": "0.0.0",
  "repository": "https://github.com/CityOfBoston/311.git",
  "license": "CC0-1.0",
  "engines": {
    "node": "8.1.0"
  },
  "scripts": {
    "dev": "babel-watch -D -w server server",
    "test": "jest",
    "build": "gulp build",
    "heroku-postbuild": "gulp build",
    "start": "node build/server",
    "storybook": "gulp storybook:head && start-storybook -p 9001 -s . -c storybook",
    "lint": "eslint --fix .",
    "test-ci": "jest --coverage --runInBand",
    "snapshot": "gulp storybook:head && build-storybook -c storybook && percy-storybook --widths=320,840,1280",
    "searchbox-load": "babel-node ./scripts/searchbox-load",
    "create-elasticsearch-index": "babel-node ./scripts/create-elasticsearch-index",
    "clear-babel-cache": "rm -rf node_modules/.cache/babel-loader",
    "precommit": "lint-staged",
    "prepush": "flow --quiet && jest --silent --no-cache --noStackTrace --lastCommit"
  },
  "lint-staged": {
    "*.js": "eslint --quiet"
  },
  "jest": {
    "resetMocks": true,
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/.next/",
      "<rootDir>/coverage/"
    ],
    "collectCoverageFrom": [
      "{components,data,lib,pages,server}/**/*\\.js",
      "!**/*.stories.js"
    ],
    "coverageReporters": [
      "lcov",
      "json"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "boom": "^5.1.0",
    "classlist-polyfill": "^1.2.0",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "elasticsearch": "^13.0.1",
    "fs-extra": "^3.0.1",
    "glamor": "^2.20.22",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.1",
    "graphql": "^0.10.1",
    "graphql-server-hapi": "^0.9.0",
    "graphql-tools": "^1.0.0",
    "hapi": "^16.1.1",
    "hapi-accept-language": "^1.0.1",
    "html5-history-api": "^4.2.8",
    "http-status": "^1.0.1",
    "https-proxy-agent": "^1.0.0",
    "inert": "^4.2.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.38.0",
    "mapbox-gl-supported": "^1.2.0",
    "mapbox.js": "^3.0.1",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.7",
    "mobx-utils": "^2.0.1",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "next": "beta",
    "nprogress": "^0.2.0",
    "opbeat": "^4.14.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.13.1",
    "react-input-mask": "^0.9.0",
    "react-scoped-error-component": "^0.9.1",
    "redbox-react": "^1.3.6",
    "sitemap": "^1.12.0",
    "svg4everybody": "^2.1.8",
    "url-search-params": "^0.9.0",
    "url-search-params-polyfill": "^1.3.0",
    "velocity-animate": "^1.5.0",
    "velocity-react": "^1.3.0"
  },
  "devDependencies": {
    "@percy-io/in-percy": "^0.1.2",
    "@percy-io/react-percy-storybook": "^1.0.0",
    "@storybook/addon-actions": "^3.1.5",
    "@storybook/react": "3.1.6",
    "apollo-codegen": "^0.12.8",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-inline-environment-variables": "^0.1.1",
    "babel-preset-latest": "^6.22.0",
    "babel-watch": "^2.0.6",
    "cheerio": "^0.22.0",
    "codecov": "^2.1.0",
    "del": "^3.0.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "fake-xml-http-request": "^1.4.0",
    "fetch-mock": "^5.9.4",
    "flow-bin": "^0.47.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-ignore": "^2.0.2",
    "gulp-plumber": "^1.1.0",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-uglify": "^3.0.0",
    "husky": "^0.14.0",
    "jest": "^20.0.0",
    "jsverify": "^0.8.0",
    "leaflet": "1.0.3",
    "lint-staged": "^4.0.0",
    "prompt": "^1.0.0",
    "pump": "^1.0.2",
    "react-proxy": "https://github.com/finneganh/react-proxy/releases/download/fix-get-calling/react-proxy-3.0.0-alpha.1.tgz",
    "react-test-renderer": "^15.5.4",
    "svg-sprite": "^1.3.6",
    "webpack-bundle-analyzer": "^2.3.0"
  }
}
