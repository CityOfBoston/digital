{
  "name": "services-js.registry-certs",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfBoston/registry-certs.git"
  },
  "license": "CC0-1.0",
  "scripts": {
    "prebuild": "rimraf build && yarn run generate-graphql-schema && yarn run generate-graphql-types",
    "dev": "yarn run prebuild && tsc-watch -p tsconfig.server.json --onSuccess \"yarn run start\"",
    "pretest": "yarn run prebuild && tsc --noEmit",
    "test": "jest",
    "percy": "scripts/percy.js",
    "watch-dependencies": "lerna run --parallel --scope registry-certs --include-filtered-dependencies watch",
    "build": "concurrently \"yarn run build:server\" \"yarn run build:next\"",
    "build:server": "tsc -p tsconfig.server.json",
    "build:next": "next build .",
    "prepare": "yarn run build",
    "start": "node build/server",
    "storybook": "start-storybook -p 9001 -s static",
    "codebuild-deploy": "npx -p ../../deploy-tools.tgz codebuild-service-deploy Dockerfile",
    "prepare-deploy": "build-storybook -s static",
    "postdeploy": "ts-node ./scripts/post-deploy",
    "clear-babel-cache": "rm -rf node_modules/.cache/babel-loader",
    "generate-graphql-schema": "mkdir -p graphql && ts2gql server/graphql/index.ts > graphql/schema.graphql",
    "generate-graphql-types": "npx apollo codegen:generate --includes=\"client/queries/*.ts\" --localSchemaFile=graphql/schema.graphql --target=typescript --outputFlat --no-addTypename client/queries/graphql-types.ts",
    "generate-fixtures": "ts-node ./scripts/generate-fixtures"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/lib/test/jest-polyfills.js",
      "<rootDir>/lib/test/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/scripts/"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.1.2",
    "@cityofboston/hapi-common": "^0.0.0",
    "@cityofboston/hapi-next": "^0.0.0",
    "@cityofboston/mssql-common": "^0.0.0",
    "@cityofboston/next-client-common": "^0.0.0",
    "@cityofboston/react-fleet": "^0.0.0",
    "@cityofboston/srv-decrypt-env": "^0.0.0",
    "@emotion/core": "^10.0.10",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "address-rfc2822": "^2.0.3",
    "apollo-server-hapi": "^2.5.0",
    "aws-sdk": "^2.100.0",
    "babel-runtime": "^6.26.0",
    "boom": "^7.2.0",
    "clear-require": "^2.0.0",
