    "core-js": "^2.6.4",
    "dataloader": "^1.3.0",
    "dotenv": "^5.0.0",
    "emotion": "^10.0.9",
    "emotion-server": "^10.0.9",
    "formik": "^1.4.2",
    "good": "^8.1.1",
    "graphql": "^14.3.0",
    "handlebars": "^4.0.11",
    "hapi": "^17.5.2",
    "http-status": "^1.0.1",
