{
  "name": "registry-certs",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfBoston/registry-certs.git"
  },
  "license": "CC0-1.0",
  "scripts": {
    "prebuild": "rimraf build && npm run generate-graphql-schema && npm run generate-graphql-types",
    "dev": "npm run prebuild && tsc-watch -p tsconfig.server.json --onSuccess \"npm run start\"",
    "pretest": "tsc --noEmit",
    "test": "scripts/test.js",
    "watch-dependencies": "lerna run --parallel --scope registry-certs --include-filtered-dependencies watch",
    "build": "concurrently \"npm run build:server\" \"npm run build:next\"",
    "build:server": "tsc -p tsconfig.server.json",
    "build:next": "next build .",
    "prepare": "npm run build",
    "start": "node build/server",
    "storybook": "start-storybook -p 9001 -s static",
    "travis-deploy": "travis-service-deploy Dockerfile",
    "prepare-deploy": "build-storybook -s static",
    "clear-babel-cache": "rm -rf node_modules/.cache/babel-loader",
    "generate-graphql-schema": "mkdir -p graphql && ts2gql server/graphql/index.ts > graphql/schema.graphql",
    "generate-graphql-types": "apollo-codegen generate client/queries/*.ts --project-name registry-certs --target typescript --output client/queries/graphql-types.d.ts && prettier --write client/queries/graphql-types.d.ts",
    "generate-fixtures": "ts-node ./scripts/generate-fixtures"
  },
  "jest": {
    "preset": "@cityofboston/config-jest-babel",
    "collectCoverage": false,
    "setupFiles": [
      "<rootDir>/lib/test/jest-polyfills.js",
      "<rootDir>/lib/test/setup.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.0.0",
    "@cityofboston/hapi-common": "^0.0.0",
    "@cityofboston/next-client-common": "^0.0.0",
    "@cityofboston/srv-decrypt-env": "^0.0.0",
    "address-rfc2822": "^2.0.3",
    "apollo-server-hapi": "^1.4.0",
    "aws-sdk": "^2.100.0",
    "babel-runtime": "^6.26.0",
    "boom": "^7.2.0",
    "clear-require": "^2.0.0",
    "compression": "^1.6.2",
    "dataloader": "^1.3.0",
    "dotenv": "^5.0.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "handlebars": "^4.0.11",
    "hapi": "^17.5.2",
    "http-status": "^1.0.1",
    "https-proxy-agent": "^2.0.0",
    "inert": "^5.1.0",
    "isomorphic-fetch": "^2.2.1",
    "mjml": "^4.0.0",
    "mobx": "^4.3.0",
    "mobx-react": "^5.2.0",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "mssql": "^4.2.1",
    "next": "7.0.0",
    "node-cleanup": "^2.1.2",
    "nprogress": "^0.2.0",
    "postmark": "^1.5.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-input-mask": "^1.2.2",
    "react-proxy": "https://github.com/finneganh/react-proxy/releases/download/fix-get-calling/react-proxy-3.0.0-alpha.1.tgz",
    "rollbar": "^2.4.1",
    "stripe": "^5.3.0",
    "styled-jsx": "^2.2.1",
    "url-search-params": "^0.10.0",
    "validatorjs": "^3.13.5",
    "velocity-react": "^1.3.3",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@cityofboston/config-babel": "^0.0.0",
    "@cityofboston/config-jest-babel": "^0.0.0",
    "@cityofboston/config-typescript": "^0.0.0",
    "@cityofboston/graphql-typescript": "^0.0.0",
    "@cityofboston/mssql-typescript": "^0.0.0",
    "@cityofboston/percy-common": "^0.0.0",
    "@percy-io/percy-storybook": "^1.4.1",
    "@storybook/addon-actions": "4.0.0-alpha.22",
    "@storybook/addon-storyshots": "4.0.0-alpha.22",
    "@storybook/addons": "4.0.0-alpha.22",
    "@storybook/react": "4.0.0-alpha.22",
    "@types/mssql": "^4.0.10",
    "@types/stripe-v3": "^3.0.8",
    "@types/styled-jsx": "^2.2.3",
    "@zeit/next-typescript": "1.1.1",
    "apollo-codegen": "0.20.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cheerio": "^0.22.0",
    "codecov": "^2.3.1",
    "concurrently": "^3.5.1",
    "del": "^3.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint-plugin-graphql": "^2.1.1",
    "faker": "^4.1.0",
    "fetch-mock": "^5.12.2",
    "jest": "^22.1.4",
    "json-loader": "^0.5.4",
    "lint-staged": "^4.0.0",
    "mockdate": "^2.0.2",
    "prettier": "^1.12.0",
    "prop-types": "^15.5.10",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "tsc-watch": "^1.0.26",
    "typescript": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.1"
  }
}
