{
  "presets": [
    ["@babel/preset-env", { "modules": "commonjs" }],
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-transform-runtime", { "polyfill": false, "regenerator": true }]
  ]  
}
