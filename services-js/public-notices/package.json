{
  "name": "public-notices",
  "version": "0.0.0",
  "private": true,
  "license": "CC0-1.0",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "next build src",
    "dev": "next dev src",
    "storybook": "start-storybook -p 9001 -s src/static",
    "watch-dependencies": "lerna run --parallel --scope public-notices --include-filtered-dependencies watch",
    "prepare": "npm run build",
    "start": "next start",
    "pretest": "npm run prebuild && tsc --noEmit",
    "test": "scripts/test.js",
    "predeploy": "rimraf export && yarn build && next export src -o export && cp -r build/.next/BUILD_ID export",
    "codebuild-deploy": "s3-apps-deploy export"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/scripts/"
    ]
  },
  "dependencies": {
    "@cityofboston/react-fleet": "^0.0.0",
    "core-js": "^2.5.7",
    "date-fns": "^1.29.0",
    "dotenv": "^5.0.0",
    "emotion": "9.2.10",
    "emotion-server": "9.2.10",
    "mobx": "^4.3.0",
    "mobx-react": "^5.2.0",
    "mobx-utils": "^4.0.0",
    "next": "7.0.2",
    "react": "16.8.1",
    "react-dom": "16.8.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@cityofboston/config-babel": "^0.0.0",
    "@cityofboston/config-typescript": "^0.0.0",
    "@percy-io/percy-storybook": "^1.4.1",
    "@storybook/addon-actions": "4.0.4",
    "@storybook/addon-storyshots": "4.0.4",
    "@storybook/addons": "4.0.4",
    "@storybook/react": "4.0.4",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.7.6",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-storyshots": "^3.4.1",
    "@types/storybook__react": "^3.0.7",
    "@zeit/next-typescript": "1.1.1",
    "babel-core": "^7.0.0-0",
    "jest": "24.1.0",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.5",
    "tsc-watch": "^1.0.26",
    "typescript": "^3.3.1"
  }
}
