{
  "name": "internal-slack-bot",
  "version": "0.0.1",
  "description": "Internal Slack utility bot",
  "private": true,
  "scripts": {
    "dev": "npm run prebuild && tsc-watch --onSuccess \"npm run start\"",
    "pretest": "tsc --noEmit",
    "test": "jest",
    "prebuild": "rimraf build",
    "build": "tsc",
    "start": "node build/server",
    "prepare": "npm run build",
    "travis-deploy": "travis-service-deploy Dockerfile",
    "tunnel": "npx ngrok http 8000"
  },
  "license": "CC0-1.0",
  "jest": {
    "preset": "@cityofboston/config-jest-typescript",
    "collectCoverage": false
  },
  "dependencies": {
    "@cityofboston/hapi-common": "^0.0.0",
    "@cityofboston/react-fleet": "^0.0.0",
    "@cityofboston/srv-decrypt-env": "^0.0.0",
    "@slack/client": "^4.8.0",
    "@slack/events-api": "^2.1.1",
    "@slack/interactive-messages": "^1.0.2",
    "aws-sdk": "^2.345.0",
    "babel-runtime": "^6.26.0",
    "boom": "^7.2.0",
    "dotenv": "^6.1.0",
    "hapi": "^17.6.0",
    "ngrok": "^3.1.0",
    "node-cleanup": "^2.1.2",
    "node-fetch": "^2.2.0",
    "rollbar": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@cityofboston/config-babel": "^0.0.0",
    "@cityofboston/config-jest-typescript": "^0.0.0",
    "@cityofboston/config-typescript": "^0.0.0",
    "@cityofboston/deploy-tools": "^0.0.0",
    "@types/boom": "^7.2.1",
    "@types/node": "^10.12.1",
    "babel-core": "^6.26.3",
    "codecov": "^3.1.0",
    "del": "^3.0.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.1.1",
    "tsc-watch": "^1.0.30",
    "typescript": "^3.1.4"
  }
}
